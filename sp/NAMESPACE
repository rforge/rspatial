useDynLib(sp)

export(CRS, print.CRS, proj4string, "proj4string<-", is.projected)

export(bbox, getgridindex, points2grid, bpy.colors, bubble, cplot, lplot, 
	mapasp, point.in.polygon, map.to.lev, zerodist, remove.duplicates)

export(coordinates, "coordinates<-", coordinates.SPDF)

export(Coordinates)

export(dimensions, "[.SpatialPointsDataFrame", "[.SpatialCellDataFrame")
 
export(names.SpatialPointsDataFrame)

#export(names.SpatialCellDataFrame)

