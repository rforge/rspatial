	as.SpatialPoints.SpatialPointsDataFrame)

export(gridparameters, "gridded<-", griddedfn, gridded)

export(plot.SpatialCell, plot.SpatialPoints, plot.SpatialCellDataFrame, 
	plot.SpatialPointsDataFrame, plot.Spatial, plot.SpatialRings)

export(as.SpatialRings.Shapelist, as.SpatialRings.matrixList, 
	as.Ring4.matrix, SpatialRings)

export(summary.Spatial, print.summary.Spatial, summary.SpatialGridded, 
	print.summary.SpatialGridded, summary.SpatialCell, 
	print.summary.SpatialCell, summary.SpatialCellDataFrame, 
	print.summary.SpatialCellDataFrame, print.SpatialPointsDataFrame, 
	dim.SpatialPointsDataFrame, ShowSpatialPointsDataFrame, 
	summary.SpatialPointsDataFrame, print.summary.SpatialPointsDataFrame, 
	print.SpatialPoints)

export(image.SpatialCellDataFrame, as.image.SpatialCellDataFrame, 
	select.spatial, stack.SpatialPointsDataFrame, 
	stack.SpatialCellDataFrame)

exportClasses(CRS, Spatial, SpatialPointsDataFrame, SpatialPoints)

exportClasses(SpatialGridded, SpatialCell, SpatialCellDataFrame)

exportClasses(SLine, SLines, SpatialLines)

exportClasses(Ring4, SpatialRings, SpatialRingsDataFrame)

exportMethods(show, bbox, coordinates, arcs, rings, gridded, dimensions, "[")

exportMethods(coerce)

exportMethods(Coordinates)

